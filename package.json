{
  "name": "stabilis-diversification",
  "version": "1.0.0",
  "description": "Stabilis Diversification Implementation Project (Nov 2025 - Mar 2027)",
  "scripts": {
    "prebuild": "powershell -ExecutionPolicy Bypass -File ./scripts/ultrathink.ps1",
    "build": "echo Building project artifacts...",
    "start": "node server.js",
    "status": "powershell -ExecutionPolicy Bypass -File ./scripts/project-status.ps1",
    "dashboard": "powershell -Command \"Get-Content .\\PROJECT-DASHBOARD.md\"",
    "phase1": "powershell -Command \"Get-Content .\\phases\\PHASE1-MOBILISATION.md\"",
    "phase2": "powershell -Command \"Get-Content .\\phases\\PHASE2-PILOT.md\"",
    "phase3": "powershell -Command \"Get-Content .\\phases\\PHASE3-ROLLOUT.md\"",
    "phase4": "powershell -Command \"Get-Content .\\phases\\PHASE4-OPTIMISATION.md\"",
    "phase5": "powershell -Command \"Get-Content .\\phases\\PHASE5-CONSOLIDATION.md\"",
    "project": "powershell -ExecutionPolicy Bypass -File ./project.ps1",
    "complete": "powershell -ExecutionPolicy Bypass -File ./project.ps1 complete",
    "reschedule": "powershell -ExecutionPolicy Bypass -File ./project.ps1 reschedule",
    "by-role": "powershell -ExecutionPolicy Bypass -File ./project.ps1 by-role",
    "next": "powershell -ExecutionPolicy Bypass -File ./project.ps1 next",
    "agenda": "powershell -ExecutionPolicy Bypass -File ./project.ps1 agenda"
